{
  "parsers": [
    {
      "name": "科技",
      "parsers": [
        {
          "name": "IT之家",
          "code": "tel.ithome",
          "type": "PAGE",
          "url": "https://www.ithome.com/",
          "parser": "$('ul.nl li').map(function() {return {title: $(this).children('a').text(), url: $(this).children('a').attr('href')}}).toArray().slice(0, 90)",
          "icon": "https://img.ithome.com/www/ithome.logo.svg"
        },
        {
          "name": "CnBeta",
          "code": "tel.cnbeta",
          "type": "PAGE",
          "url": "https://www.cnbeta.com/",
          "parser": "$('dl').map(function(){return {title:$(this).children('dt').text(),summary:$(this).children('dd').text(),image:$(this).find('img').attr('data-original'),url:$(this).children('dt').children('a').attr('href')}}).toArray()",
          "icon": "https://www.cnbeta.com/images/logo_1.png"
        },
        {
          "name": "DoNews",
          "code": "tel.donews",
          "type": "PAGE",
          "url": "https://www.donews.com/",
          "parser": "$('.left a.news-item').map(function() {return {title:$(this).find('.title').text(), summary:$(this).find('.desc').text(), image:$(this).find('img').attr('src'), url:$(this).attr('href')}}).toArray()",
          "icon": "https://www.donews.com/static/2020824new/images/logo.png"
        }
      ]
    },
    {
      "name": "综合",
      "parsers": [
        {
          "name": "腾讯网",
          "code": "tencent",
          "type": "PAGE",
          "url": "https://news.qq.com/",
          "parser": "$('ul.list li.itme-ls').map(function() { return {title: $(this).find('h3>a').text(), url: $(this).find('h3>a').attr('href'), image: $(this).find('.picture').attr('src')}}).toArray()",
          "icon": "https://mat1.gtimg.com/pingjs/ext2020/newom/build/static/images/new_logo.png"
        }
      ]
    }
  ]
}